---
import Layout from '@layouts/Layout.astro'
import LoremIpsum from '@components/widgets/LoremIpsum.astro'
export async function getStaticPaths() {
  const pages = [
    {
      lang: undefined,
      title: 'Astro Default',
      text: 'Welcome to the Astro store!'
    },
    {
      lang: 'en',
      title: 'Astro English',
      text: 'We have lots of products for you'
    },
    {
      lang: 'es',
      title: 'Astro Spanish',
      text: 'If you want to learn Astro, you must read this book.'
    }
  ]
  return pages.map(({ lang, title, text }) => {
    return {
      params: { lang },
      props: { title, text }
    }
  })
}
---

<Layout title="About" subtitle="About subtitle" details="About details">
  <main>
    About content
    <br /><br />
    <LoremIpsum count={24} />
  </main>
</Layout>

<style></style>

